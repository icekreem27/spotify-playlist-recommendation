    "from sklearn.metrics.pairwise import cosine_similarity"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 168,
   "id": "9b061f55",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Pull songs from a specific playlist.\n",
    "def generate_playlist_recos(df, features, nonplaylist_features, top_n=40):\n",
    "    # Filter out non-playlist tracks based on their IDs\n",
    "    non_playlist_df = df[df['id'].isin(nonplaylist_features['id'].values)].copy()\n",
    "    \n",
    "    # Calculate similarity using cosine similarity and add as a new column\n",
    "    non_playlist_df['sim'] = cosine_similarity(nonplaylist_features.drop('id', axis=1).values, features.values.reshape(1, -1))[:, 0]\n",
    "\n",
    "    # Sort by similarity and pick the top_n\n",
    "    non_playlist_df_top = non_playlist_df.sort_values('sim', ascending=False).head(top_n)\n",
    "    \n",
    "    # Fetch URLs for the top tracks using Spotify API\n",
    "    non_playlist_df_top['url'] = non_playlist_df_top['id'].apply(lambda x: sp.track(x)['album']['images'][1]['url'])\n",
    "    \n",
    "    # Select only the specified columns to return\n",
    "    columns_to_keep = ['id', 'name', 'artists_updated', 'sim', 'url']\n",
    "    final_output = non_playlist_df_top.loc[:, columns_to_keep]\n",
    "    \n",
    "    return final_output\n"
   ]
  },
